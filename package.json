{
	"name": "webcod",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "npx cross-env NODE_ENV=development&& npx prisma generate && nodemon ./index.ts",
		"start": "npx cross-env NODE_ENV=production&& npx prisma generate && ts-node ./index.ts",
		"prisma-generate": "npx prisma generate",
		"prisma-introspect": "npx dotenv -e .env -e .env.development -e ./src/prisma/prisma.env -- npx prisma introspect",
		"prisma-studio": "npx dotenv -e .env -e .env.development -e ./src/prisma/prisma.env -- npx prisma studio",
		"make-admin": "ts-node ./scripts/makeAdmin.ts"
	},
	"dependencies": {
		"@prisma/client": "^2.18.0",
		"bcrypt": "^5.0.1",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"crypto-random-string": "^3.3.1",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"dotenv-expand": "^5.1.0",
		"express": "~4.16.1",
		"express-session": "^1.17.1",
		"express-validator": "^6.10.0",
		"helmet": "^4.4.1",
		"http-errors": "~1.6.3",
		"morgan": "~1.9.1",
		"mysql2": "^2.2.5",
		"passport": "^0.4.1",
		"passport-local": "^1.0.0",
		"rcon-srcds": "^2.0.1"
	},
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/connect-flash": "0.0.36",
		"@types/cors": "^2.8.10",
		"@types/express": "^4.17.11",
		"@types/express-session": "^1.17.3",
		"@types/mysql": "^2.15.17",
		"@types/node": "^14.14.31",
		"@types/passport": "^1.0.6",
		"@types/passport-local": "^1.0.33",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^4.0.0",
		"nodemon": "^2.0.7",
		"prisma": "^2.18.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.5"
	},
	"prisma": {
		"schema": "./src/prisma/schema.prisma"
	}
}
