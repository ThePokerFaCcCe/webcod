{
	"name": "webcod",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "cross-env-shell NODE_ENV=development \"sequelize db:migrate && prisma generate && nodemon ./index.ts\"",
		"start": "cross-env-shell NODE_ENV=production \"sequelize db:migrate && prisma generate && ts-node ./index.ts\"",
		"prisma-generate": "prisma generate",
		"prisma-introspect": "dotenv -e .env -e .env.development -e ./src/prisma/prisma.env -- prisma introspect",
		"prisma-studio": "dotenv -e .env -e .env.development -e ./src/prisma/prisma.env -- prisma studio",
		"make-admin": "ts-node ./scripts/makeAdmin.ts"
	},
	"dependencies": {
		"@prisma/client": "^2.19.0",
		"bcrypt": "^5.0.1",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"crypto-random-string": "^3.3.1",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"dotenv-expand": "^5.1.0",
		"express": "~4.16.1",
		"express-session": "^1.17.1",
		"express-validator": "^6.10.0",
		"gamedig": "^3.0.0",
		"helmet": "^4.4.1",
		"http-errors": "~1.6.3",
		"lodash": "^4.17.21",
		"morgan": "~1.9.1",
		"mysql2": "^2.2.5",
		"passport": "^0.4.1",
		"passport-local": "^1.0.0",
		"rcon-srcds": "^2.0.1",
		"source-server-query": "^1.0.1"
	},
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/cors": "^2.8.10",
		"@types/express": "^4.17.11",
		"@types/express-session": "^1.17.3",
		"@types/gamedig": "^3.0.0",
		"@types/lodash": "^4.14.168",
		"@types/node": "^14.14.31",
		"@types/passport": "^1.0.6",
		"@types/passport-local": "^1.0.33",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^4.0.0",
		"nodemon": "^2.0.7",
		"prisma": "^2.19.0",
		"sequelize": "^6.5.0",
		"sequelize-cli": "^6.2.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.3"
	},
	"prisma": {
		"schema": "./src/prisma/schema.prisma"
	}
}
